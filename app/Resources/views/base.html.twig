<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {%  stylesheets
                'bootstrap-master/dist/css/bootstrap.css'
                'bootstrap-master/dist/css/bootstrap-theme.css'
                'bootstrap-master/dist/css/app.css'
                filter='cssrewrite'
            %}
            <link rel="stylesheet" type="text/css" href="{{  asset_url }}">
            {% endstylesheets %}
            <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
            <link href="http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
            <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        {% endblock %}
        {% block headerScripts %}
            {% javascripts
                '../app/Resources/js/vendor/modernizr-latest.js'
                '../app/Resources/js/vendor/jquery-2.1.1.js'
                '../app/Resources/js/vendor/jquery.easing.1.3.js'
                'bootstrap-master/dist/js/bootstrap.js'
                'bootstrap-master/dist/js/app.js'
            %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
        {% include 'navbar.html.twig' %}

        {% block body %}{% endblock %}

        {% block javascripts %}{% endblock %}

        {% include 'footer.html.twig' %}
    </body>

</html>